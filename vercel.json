{
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" },
      { "src": "/login", "dest": "/"},
      { "src": "/menu", "dest": "/"},
      { "src": "/producto/:id", "dest": "/" },
      { "src": "/carrito", "dest": "/" },
      { "src": "/cart", "dest": "/" },
      { "src": "/checkout", "dest": "/" },
      { "src": "/admin", "dest": "/" },
      { "src": "/**", "dest": "/" }
    ]
}